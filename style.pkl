hidden pmtiles = "pmtiles://https://data.source.coop/smartmaps/foil4gr1/reusable-h3-pmtiles/h3.pmtiles"
hidden layer = "h3"

version = 8
sources {
  a { type = "vector" url = pmtiles attribution = "Reusable H3" }
}
layers = new Listing {
  new {
    id = "background" type = "background"
    paint { `background-color` = "#222" }
  }
  new {
    id = layer type = "fill" source = "a"
    `source-layer` = layer
    paint {
      `fill-color` = "#0ff"
      `fill-outline-color` = "#000"
    }
  }
}
